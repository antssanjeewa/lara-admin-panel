{
    "name": "antssanjeewa/lara-admin-panel",
    "description": "Template for newly created project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sameera Sanjeewa",
            "email": "antssanjeewa94@gmail.com"
        }
    ],

    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0"
    },

    "autoload": {
        "psr-4": {
            "Antssanjeewa\\LaraAdminPanel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antssanjeewa\\LaraAdminPanel\\AdminPanelServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antssanjeewa\\LaraAdminPanel\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}